                <div class="card-action">
                  <a href="#"><i class="fa fa-calendar" aria-hidden="true"></i>
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>

<%# ===================================%>
<%# user option to edit his/her info%>
<%# ===================================%>


<div class="btn-group open">
  <a class="btn btn-primary" href="#"><i class="fa fa-user fa-fw"></i> User</a>
